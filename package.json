{
  "name": "@mattstyles/temple",
  "shortname": "temple",
  "version": "1.5.0.rc1",
  "description": "Creates files from templates",
  "main": "lib/index.js",
  "bin": {
    "temple": "lib/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard",
    "flow": "flow",
    "build": "babel src -d lib",
    "watch": "watch 'flow && npm run build' src",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "flow"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/temple.git"
  },
  "keywords": [
    "template",
    "file",
    "scaffold"
  ],
  "author": "Matt Styles <matt@veryfizzyjelly.com> (http://github.com/mattstyles)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstyles/temple/issues"
  },
  "homepage": "https://github.com/mattstyles/temple#readme",
  "dependencies": {
    "@mattstyles/alias": "^1.0.0",
    "app-root-dir": "^1.0.2",
    "cli-table": "^0.3.1",
    "configstore": "^2.1.0",
    "errno": "^0.1.4",
    "inquirer": "^1.2.3",
    "mkdirp": "^0.5.1",
    "xdg-basedir": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.49.1",
    "standard": "^10.0.2"
  }
}
